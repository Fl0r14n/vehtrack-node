{
  "development": {
    "database": {
      "dialect": "sqlite",
      "storage": "./db.development.sqlite",
      "define": {
        "freezeTableName": true
      },
      "sync": {
        "force": true
      },
      "syncOnAssociation": true
    },
    "jwt": {
      "token": {
        "secret": "hackme",
        "expiresIn": 500
      },
      "refreshToken": {
        "secret": "whatever",
        "expiresIn": 604800
      }
    }
  },
  "test": {
    "database": {
      "dialect": "sqlite",
      "storage": ":memory:"
    },
    "jwt": {
      "token": {
        "secret": "hackme",
        "expiresIn": 30
      },
      "refreshToken": {
        "secret": "whatever",
        "expiresIn": 60
      }
    }
  },
  "production": {
    "database": {
      "database": "tc",
      "username": "tc",
      "password": "tc",
      "host": "localhost",
      "port": 5432,
      "dialect": "postgres"
    }
  }
}
